generator client {
  provider = "prisma-client-js"
//  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Users {
  id Int @id @default(autoIncrement())
  user_name String 
  password String @unique
  email String @unique
  address String?
  phone_number String?

}
